    "karma": "karma",
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack --progress --profile ",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "clean:dist": "npm run rimraf -- dist",
    "clean:dll": "npm run rimraf -- dll",
    "clean:cache": "npm run rimraf -- .awcache",
    "clean:electron": "npm run rimraf -- build",
    "build:electron": "npm run build:electron.full",
    "prebuild:electron.full": "npm run clean:electron; npm run clean:cache",
    "build:electron.full": "npm run build:electron.renderer && npm run build:electron.main",
    "postbuild:electron.full": "npm run electron:start",
    "build:electron.renderer": "npm run webpack -- --config config/webpack.renderer.prod.js",
    "build:electron.main": "npm run webpack -- --config config/webpack.electron.prod.js",
    "clean:build": "rimraf ./build/*",
    "test:once": "karma start karma.conf.js --single-run",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64  --prune=true --out=release-builds --version-string.CompanyName=SteveMieskoskiInc --version-string.FileDescription=CE --version-string.ProductName=\"Advisor\"",
    "package-winTwo": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64  --prune=true --out=release-builds --version-string.CompanyName=SteveMieskoskiInc --version-string.FileDescription=CE --version-string.ProductName=\"Advisor\"",
    "package-linux": "electron-packager ./build --overwrite --platform=linux --arch=x64 --icon=assets/icons/favicon-96x96.png --prune=true --out=release-builds",
    "preelectron:bundleLinux": "npm run clean:build",
    "electron:bundleLinux": " webpack --config config/webpack.renderer.config.js ; webpack --config config/webpack.main.config.js",
    "electronMain:bundleLinux": "webpack --config config/webpack.main.config.js",
    "electronRenderer:bundleLinux": " webpack --config config/webpack.renderer.config.js",
    "start:electron": "electron ./build",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
